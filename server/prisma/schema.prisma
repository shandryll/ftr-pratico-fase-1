generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Shortlink {
  id            String   @id @default(uuid())
  originalUrl   String
  shortenedUrl  String
  accessCounter Int      @default(0)
  created_at    DateTime @default(now())

  @@map("shortlinks")
}
